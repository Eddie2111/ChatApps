{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["LF"],
        "max-len": ["error", {"code": 120}],
        "indent": ["error", 4],
        "no-undef": "off",
        "no-unused-vars": "off"
        // "no-tabs": "off",
        // "no-invalid-this": "off",
        // "no-prototype-builtins": "off",
        // "no-async-promise-executor": "off"

    }
}
